    "eslint-plugin-react-hooks": "^1.0.2"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "commitizen": "^3.1.1",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^5.16.0",
    "eslint-plugin-babel": "^5.3.0",
